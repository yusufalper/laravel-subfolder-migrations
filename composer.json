{
    "name": "yusufalper/laravel-subfolder-migrations",
    "description": "Organize your migrations into subfolders.",
    "keywords": [
        "yusufalper",
        "laravel",
        "laravel-subfolder-migrations"
    ],
    "homepage": "https://github.com/yusufalper/laravel-subfolder-migrations",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusuf Alper SarÄ±",
            "email": "yusufalpersari@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^9|^10",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0|^10"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Yusufalper\\LaravelSubfolderMigrations\\": "src",
            "Yusufalper\\LaravelSubfolderMigrations\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yusufalper\\LaravelSubfolderMigrations\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yusufalper\\LaravelSubfolderMigrations\\LaravelSubfolderMigrationsServiceProvider"
            ],
            "aliases": {
                "LaravelSubfolderMigrations": "Yusufalper\\LaravelSubfolderMigrations\\Facades\\LaravelSubfolderMigrations"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
